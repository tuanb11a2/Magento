<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
<head>
    <title>Helloworld Landingspage</title>
</head>
<body>
    <referenceBlock name="catalog.compare.sidebar" remove="true"/>

    <referenceBlock name="wishlist_sidebar" remove="true"/>

    <referenceContainer name="content">
        <block class="Packt\HelloWorld\Block\Landingspage" name="landingsblock" template="Packt_HelloWorld::landingspage.phtml" />
    </referenceContainer>

    <referenceContainer name="content">
        <block class="Packt\HelloWorld\Block\Newproducts" name="newproductsblock" template="Packt_HelloWorld::newproducts.phtml" />
    </referenceContainer>
</body>
</page>